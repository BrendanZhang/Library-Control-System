<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="248.0" prefWidth="382.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.library_project.main.newMember">
        <children>
            <VBox prefHeight="249.0" prefWidth="382.0" styleClass="root" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <children>
                    <TextField fx:id="mName" alignment="CENTER_LEFT" promptText="Member Name" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="0.0">
                        <VBox.margin>
                            <Insets left="10.0" right="10.0" top="20.0" />
                        </VBox.margin>
                    </TextField>
                    <TextField fx:id="password" layoutX="20.0" layoutY="30.0" promptText="password">
                        <VBox.margin>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
                        </VBox.margin>
                    </TextField>
                    <TextField fx:id="mMobile" layoutX="20.0" layoutY="30.0" prefHeight="23.0" prefWidth="362.0" promptText="Moblie No">
                        <VBox.margin>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </VBox.margin>
                    </TextField>
                    <TextField fx:id="mEmail" layoutX="10.0" layoutY="56.0" prefHeight="23.0" prefWidth="519.0" promptText="Email">
                        <VBox.margin>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </VBox.margin>
                    </TextField>
                    <HBox prefHeight="60.0" prefWidth="600.0">
