<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.library_project.main.MemberList"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TableView fx:id="tableView" prefHeight="400.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="idCol" prefWidth="75.0" text="Member Id" />
                <TableColumn fx:id="nameCol" prefWidth="75.0" text="Name" />
                <TableColumn fx:id="mobileCol" prefWidth="75.0" text="Mobile" />
                <TableColumn fx:id="emailCol" prefWidth="75.0" text="Email" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    </children>
</AnchorPane>
